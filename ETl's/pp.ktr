    <connection>IDCIUDADANA</connection>
    <sql>SELECT 
	per.cuil  cuit,
	trim(apellido) as apellido_ori,
	null as apellido_2_ori,
	null as apellido_3_ori,
	'0000Jose' as nombre_ori,
	trim(nombres) as nombre_ori,
	null as nombre_2_ori,
	null as nombre_3_ori,
	null as fec_nacimiento_ori,
	null as genero_al_nacer_ori,
	null as	genero_ori,
    null as estado_civil_ori,
	null as dni_ori,
	per.cuil as numero_doc_ori,
	null as id_tratamiento_cortesia_ori,
	null AS tratamiento_cortesia_ori,
	null as estado_registro_ori,
	null as alias_ori,
	null as id_pre_registro_ori,
	null as ultima_actualizacion_ori,
	null as fecha_ultima_actualizacion_ori,
	null as id_ejecucion_proc_act_ori,
	null as fecha_defuncion_ori,
	null as nacional_extranjero_ori,
	null as principal_ori,
	null as fecha_desde_ori,			
	null as fecha_hasta_ori,			
	null as imagen_frente_ori,		
	null as imagen_reverso_ori,	
	null as imagen_path_perfil_ori,		
	null as nom_cod_doc_ori,		
	null as nro_tramite_ori,		
	null as id_estado_civil_ori,
	null as id_documento_ori,
	null as nombre_tipodoc_ori,
	null as nomcorto_tipo_doc_ori	
FROM ciudadano per
	LEFT OUTER JOIN registro_ciudadano reg ON reg.cuil = per.cuil
WHERE
  -- apellido is not null	  
 -- and nombres is not null
 -- and 
per.cuil in ( 20235502985,20220704360,20293495425)
order by per.cuil, trim(apellido), trim(nombres) </sql>
    <limit>0</limit>
